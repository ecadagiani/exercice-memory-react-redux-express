{
  "name": "pyrite-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "buildSass": "sass src/static:src/static",
    "watchSass": "sass --watch src/static:src/static",
    "startDevNode": "NODE_PATH=./src nodemon --watch src -e js,html -L src/app.js",
    "startNode": "NODE_PATH=./src node src/app.js",
    "start": "npm-run-all buildSass startNode",
    "startDev": "npm-run-all --parallel watchSass startDevNode"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "author": "Eden Cadagiani <e.cadagiani@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.0",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.20",
    "mysql": "^2.18.1",
    "pug": "^3.0.0",
    "uniqid": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.8"
  }
}
